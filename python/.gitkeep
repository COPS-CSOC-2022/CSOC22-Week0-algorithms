# Python3 program for Naive Pattern
def search(pat,text):
    l=len(pat)
    n=len(text)
    for i in range(n-l+1):
        j=0
        while(j<l):
            if (text[i+j]!=pat[j]):
                break
            j+=1
        if (j==l):
            print("Pattern found at index ",i)
if __name__=='__main__':
    text=input()
    pat=input()
    search(pat,text)
